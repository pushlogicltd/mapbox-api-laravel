{
    "name": "pushlogicltd/mapbox-api-laravel",
    "description": "A Laravel 10+ Package for managing Mapbox Datasets and Tilesets",
    "type": "library",
    "require": {
        "php": ">=8.2.0",
        "illuminate/contracts": "^10.29.0",
        "illuminate/support": "^10.29.0",
        "guzzlehttp/guzzle": "^7.2",
        "jmikola/geojson": "^1.1.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Matt Fox",
            "email": "matt.fox@thebluevertex.com"
        },
        {
            "name": "Push Logic Ltd",
            "email": "support@pushlogic.co.uk"
        }
    ],
    "autoload": {
        "psr-4": {
            "PushLogicLtd\\MapBoxAPILaravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PushLogicLtd\\MapBoxAPILaravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "PushLogicLtd\\MapBoxAPILaravel\\MapBoxAPILaravelServiceProvider"
            ],
            "aliases": {
                "Mapbox": "PushLogicLtd\\MapBoxAPILaravel\\Facades\\Mapbox"
            }
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "require-dev": {
        "orchestra/testbench": "^8.14.0",
        "phpunit/phpunit": "^10.1"
    }
}
